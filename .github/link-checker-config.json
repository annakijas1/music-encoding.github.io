{
  "ignorePatterns": [
    {
      "pattern": "^(http|https)://localhost"
    },
    {
      "pattern": "mailto:{{.*}}"
    },
    {
      "pattern": "^.github/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://music-encoding.org/"
    },
    {
      "pattern": "^../schema/",
      "replacement": "https://music-encoding.org/schema/"
    },
    {
      "pattern": "^../102_incipit.png",
      "replacement": "./102_incipit.png"
    }
  ],
  "retryOn429": true,
  "aliveStatusCodes": [200, 206]
}
